        }
    },
    "tags": [{
        "name": "Contract",
        "description": "endpoints referentes ao dominio de usuários"
    }],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "appication/json"
    ],
    "paths":{
        "/getContract": {
            "get": {
                "tags": [
                    "Contract"
                ],
                "summary": "return contracts from database",
                "responses": {
                    "200": {
                        "description": "List of contracts",
                        "content": {
                            "application/json":{
                                "schema": {
                                    "$ref": "#/definitions/Contract"
                                }
                            }
                        }
                    },
                    "5XX": {
                        "description": "Erro Inesperado"
                    }
                }
            }
        },
        "/createContract": {
            "post": {
                "tags":[
                    "Contract"
                ],
                "summary": "Insere novo contrato no banco de dados",
                "requestBody":{
                    "description": "Propriedades do contarto",
                    "required": true,
                    "content":{
                        "application/json": {
                            "schema": {
                                "$ref":"#/definitions/Contract"
                            }
                        }
                    }
                },
                "produces":[
                    "appication/json"
                ],
                "responses":{
                    "200": {
                        "description": "OK"
                    },
                    "5XX": {
                        "description": "Error"
                    }
                }
            }
        },
        "/updateContract/{id}": {
            "put": {
                "summary": "Atualiza o contrato correspondente ao ID",
                "tags": [
                    "Contract"
                ],
                "requestBody": {
                    "description": "Propriedades do contrato",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/definitions/Contract"
                            }
                        }
                    }
                },
                "parameters": [{
                    "in": "path",
                    "name": "id",
                    "required": true,
                    "description": "ID do contrato que iremos atualizar",
                    "schema": {
                        "$ref": "#/definitions/id"
                    }
                }],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/UpdateResponse"
                                }
                            }
                        }
                    },
                    "5XX": {
                        "description": "Erro inesperado."
                    }
                }
            }
        },
        "/deleteContract/{id}": {
            "delete": {
                "summary": "Deleta o contrato correspondente ao ID",
                "tags": [
                    "Contract"
                ],
                "parameters": [{
                    "in": "path",
                    "name": "id",
                    "required": true,
                    "description": "ID do contrato que iremos atualizar",
                    "schema": {
                        "$ref": "#/definitions/id"
                    }
                }],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/DeleteResponse"
                                }
                            }
                        }
                    },
                    "5XX": {
                        "description": "Erro inesperado."
                    }
                }
            }
        }
    },

    "definitions":{
        "Contract": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "status": {
                        "type":"boolean"
                    }
                }
            }
        },
        "Contracts": {
            "type":"object",
            "properties": {
                "id": {
                    "type":"number"
                }
            }
        }
    },
    "id": {
        "properties": {
