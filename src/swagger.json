                    },
                    "t": {
                        "type": "number"
                    }
                }
            },
            "electionId": {
                "type": "string"
            },
            "ok": {
                "type": "number"
            },
            "$clusterTime": {
                "type": "object",
                "properties": {
                    "clusterTime": {
                        "type": "string"
                    },
                    "signature": {
                        "type": "object",
                        "properties": {
                            "hash": {
                                "type": "string"
                            },
                            "keyId": {
                                "type": "string"
                            }
                        }
                    }
                }
            },
            "operationTime": {
                "type": "string"
            },
            "deletedCount": {
                "type": "number"
            }
        }
    }
}